CREATE TABLE derived.vector_universal_company (
    vuc_id character varying(32) NOT NULL ENCODE raw
    distkey
,
        fbf_id character varying(256) ENCODE lzo,
        pdl_id character varying(256) ENCODE lzo,
        linkedin_url character varying(256) ENCODE lzo,
        name character varying(256) ENCODE lzo,
        domain character varying(256) ENCODE lzo,
        employee_count character varying(256) ENCODE bytedict,
        industry character varying(256) ENCODE lzo,
        street_address character varying(256) ENCODE lzo,
        address_line_2 character varying(256) ENCODE lzo,
        locality character varying(256) ENCODE lzo,
    region
        character varying(256) ENCODE lzo,
        zip character varying(256) ENCODE lzo,
        country character varying(256) ENCODE lzo,
        related_domains character varying(256) ENCODE lzo,
        data_source character varying(256) ENCODE bytedict,
        dataset_version character varying(256) ENCODE bytedict,
        created_at timestamp without time zone DEFAULT ('now':: text):: timestamp with time zone ENCODE az64,
        updated_at timestamp without time zone DEFAULT ('now':: text):: timestamp with time zone ENCODE az64,
        PRIMARY KEY (vuc_id)
) DISTSTYLE KEY
SORTKEY
    (vuc_id);